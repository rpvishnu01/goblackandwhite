<!-- Product Section start -->
<section class="product ">
    <div class="row property__gallery">
        {{#each products}}
        <div class="col-lg-3 col-md-4 col-sm-6 mix women">
            <div class="product__item">

                <div class="product__item__pic set-bg  img-wrapper">
                    <a href="/product-details/{{this._id}}"> <img class="inner-img"
                            src="/product-images/{{Images.[0].filename}}" alt=""></a>

                    {{#if this.Stock }}
                    <div class="label new">OUT OF STOKE</div>

                    {{else}}
                      <ul class="product__hover   bw__controls">
                        <li><button onclick="addToCart('{{this._id}}')" class="btn_one mb-2"> ADD TO <span
                                    class="icon_bag_alt"></span></button></li>
                        <li><button onclick="addToWishlist('{{this._id}}')" class="btn_one">ADD TO <span
                                    class="icon_heart_alt"></span></button></li>

                    </ul>
                    {{/if}}
                  



                </div>



                <div class="product__item__text">
                    <h5><a href="#">{{this.ProductName}}</a></h5>
                    {{!-- <h6><a href="#">{{this.About}}</a></h6> --}}
                  
                    {{!-- <ul class="product__hover">
                        <li><a href="/product-details"><span class="arrow_expand"></span></a></li>
                        <li><a href="/add-to-wishlist/{{this._id}}"><span class="icon_heart_alt"></span></a></li>
                        <li><a href="/add-to-cart/{{this._id}}"><span class="icon_bag_alt"></span></a></li>
                    </ul> --}}


                    <div class="product__price mt-1 mb-1">
                        <h5>â‚¹{{this.Price}}/-</h5>
                    </div>





                    {{!-- <div class="bw__controls">
                        <button onclick="addToCart('{{this._id}}')" class="btn_one mb-2"> ADD TO CART</button>
                        <button onclick="addToWishlist('{{this._id}}')" class="btn_one"> ADD TO WISHLIST</button>
                    </div> --}}
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</section>
